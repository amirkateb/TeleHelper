{
  "name": "telehelper/telegram-sender",
  "description": "Advanced Laravel package to send Telegram messages using multiple bots with support for buttons, media, keyboards, and clean error handling.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "AmirMohammad KatebSaber",
      "email": "amveks43@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "TeleHelper\\TelegramSender\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TeleHelper\\TelegramSender\\TelegramSenderServiceProvider"
      ],
      "aliases": {
        "Telegram": "TeleHelper\\TelegramSender\\Facades\\Telegram"
      }
    }
  },
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^10.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}